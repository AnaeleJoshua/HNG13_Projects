generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StringAnalysis {
  id                       String   @id
  value                    String
  length                   Int
  is_palindrome             Boolean
  unique_characters         Int
  word_count                Int
  sha256_hash               String
  character_frequency_map   Json
  created_at                DateTime @default(now())
}
