{
  "name": "stage1-stringanalyser",
  "version": "1.0.0",
  "description": "A RESTful API that analyzes strings and stores computed properties (length, palindrome check, unique characters).",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prisma": "prisma"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "string-analyzer",
    "hng13"
  ],
  "author": "Anaele Joshua",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
